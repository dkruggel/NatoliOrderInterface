                                <Border Grid.Row="3">
                                    <Border Panel.ZIndex="3" BorderBrush="White" BorderThickness="1,1,0,0" CornerRadius="0">
                                        <Border.Effect>
                                            <DropShadowEffect BlurRadius="8" ShadowDepth="3" Direction="135" Color="LightGray" Opacity=".75"/>
                                        </Border.Effect>
                                    </Border>
                                </Border>
                                <DockPanel LastChildFill="False" DockPanel.Dock="Top" Height="30" Grid.Row="3">
                                    <TextBox Name="SearchTextBox" Style="{StaticResource SearchBox}" DockPanel.Dock="Left" Width="Auto" TextChanged="SearchTextBox_TextChanged"/>
                                    <StackPanel Orientation="Horizontal" DockPanel.Dock="Right">
                                        <!--<Button x:Name="SearchButton" Style="{StaticResource SearchButton}" Margin="2,0" HorizontalAlignment="Right"/>
                                        <Button x:Name="WindowButton" Style="{StaticResource WindowButton}" Margin="2,0" HorizontalAlignment="Right"/>
                                        <Button x:Name="ExportButton" Style="{StaticResource ExportButton}" Margin="2,0" HorizontalAlignment="Right"/>
                                        <Button x:Name="ArchiveButton" Style="{StaticResource ArchiveButton}" Margin="2,0" HorizontalAlignment="Right"/>
                                        <Button x:Name="ExpandButton" Style="{StaticResource ExpandButton}" Margin="2,0" HorizontalAlignment="Right"/>
                                        <Button x:Name="CollapseButton" Style="{StaticResource CollapseButton}" Margin="2,0" HorizontalAlignment="Right" Visibility="Visible"/>-->
                                    </StackPanel>
                                    <TextBlock Text="{Binding ElementName=DriveWorksQueueListBox, Path=Items.Count, StringFormat='Count: {0}'}" DockPanel.Dock="Right" Style="{StaticResource ModuleTextBlock}" HorizontalAlignment="Center" Margin="0,0,10,0"/>
                                </DockPanel>
                            </Grid>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style x:Key="NatoliOrderListModule" TargetType="Label" x:Name="NatoliOrderListModule">
            <Setter Property="Grid.IsSharedSizeScope" Value="True"/>
            <Setter Property="OverridesDefaultStyle" Value="True"/>
            <Setter Property="MaxHeight" Value="410"/>
            <Setter Property="MinWidth" Value="400"/>
            <Setter Property="MaxWidth" Value="650"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Label">
                        <Grid>
                            <Border BorderBrush="LightGray" BorderThickness="0,0,1,1" CornerRadius="3" Background="White">
                                <Border.Effect>
                                    <DropShadowEffect BlurRadius="6" ShadowDepth="3" Direction="315" Color="Black" Opacity="0.75"/>
                                </Border.Effect>
                            </Border>
                            <Border BorderBrush="White" BorderThickness="1,1,0,0" CornerRadius="3" Background="White">
                                <Border.Effect>
                                    <DropShadowEffect BlurRadius="6" ShadowDepth="3" Direction="135" Color="LightGray" Opacity="0.5"/>
                                </Border.Effect>
                            </Border>
                            <Grid x:Name="DisplayGrid">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <DockPanel x:Name="TitleDockPanel" LastChildFill="True" DockPanel.Dock="Top" Height="30" Grid.Row="0" MouseDown="DockPanel_MouseDown" MouseUp="DockPanel_MouseUp">
                                    <TextBlock Text="Natoli Order List" DockPanel.Dock="Left" Style="{StaticResource ModuleTextBlock}" FontSize="20" VerticalAlignment="Top"/>
                                    <StackPanel Orientation="Horizontal" DockPanel.Dock="Right">
                                        <Button x:Name="ExportButton" Style="{StaticResource ExportButton}" Margin="2,0" HorizontalAlignment="Right" ToolTip="Export To Excel" Click="ExportButton_Click" Visibility="Visible"/>
                                        <Button x:Name="WindowButton" Style="{StaticResource WindowButton}" Margin="2,0" HorizontalAlignment="Right" ToolTip="Open Work Order" Click="OpenOrderButton_Click"/>
                                        <Button x:Name="ExpandButton" Style="{StaticResource ExpandButton}" Click="ExpandModule_Click"/>
                                        <Button x:Name="CollapseButton" Style="{StaticResource CollapseButton}" Visibility="Visible" Click="CollapseModule_Click"/>
                                    </StackPanel>
                                    <TextBlock Text=""/>
                                </DockPanel>
                                <Border BorderBrush="{StaticResource Medium}" BorderThickness="0,0,0,3">
                                    <Border.Effect>
                                        <DropShadowEffect BlurRadius="8" ShadowDepth="1" Direction="90" Color="LightGray" Opacity="0.55"/>
                                    </Border.Effect>
                                </Border>
                                <Grid x:Name="HeaderGrid" DockPanel.Dock="Top" HorizontalAlignment="Stretch" Height="26" Background="{StaticResource Light}" Grid.Row="1">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition SharedSizeGroup="Checkbox" Width="35"/>
                                        <ColumnDefinition SharedSizeGroup="OrderNumber" Width="80"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition SharedSizeGroup="ShipDate" Width="100"/>
                                        <ColumnDefinition SharedSizeGroup="Rush" Width="55"/>
                                        <ColumnDefinition SharedSizeGroup="OnHold" Width="90"/>
                                        <ColumnDefinition SharedSizeGroup="Rep" Width="50"/>
                                    </Grid.ColumnDefinitions>
                                    <CheckBox x:Name="HeaderCheckBox" Grid.Column="0" Margin="7,0,0,0" Checked="HeaderCheckBox_Checked" Unchecked="HeaderCheckBox_Unchecked"/>
                                    <TextBlock Text="Order #" Style="{StaticResource ModuleHeaderTextBlock}" Grid.Column="1" Margin="5,0,0,0"/>
                                    <TextBlock Text="Customer" Style="{StaticResource ModuleHeaderTextBlock}" Grid.Column="2" Margin="5,0,0,0"/>
                                    <TextBlock Text="Ship Date" Style="{StaticResource ModuleHeaderTextBlock}" Grid.Column="3" Margin="-7,0,0,0"/>
                                    <TextBlock Text="Rush" Style="{StaticResource ModuleHeaderTextBlock}" Grid.Column="4" Margin="-7,0,0,0"/>
                                    <TextBlock Text="On Hold" Style="{StaticResource ModuleHeaderTextBlock}" Grid.Column="5" Margin="-7,0,0,0"/>
                                    <TextBlock Text="Rep" Style="{StaticResource ModuleHeaderTextBlock}" Grid.Column="6" Margin="-7,0,0,0"/>
                                </Grid>
                                <Border x:Name="TopBorder" Panel.ZIndex="3" Grid.Row="1" BorderBrush="{Binding Background, ElementName=HeaderGrid}" BorderThickness="0,0,0,1" CornerRadius="0">
                                    <Border.Effect>
                                        <DropShadowEffect BlurRadius="8" ShadowDepth="2" Direction="270" Color="Black" Opacity="0.55"/>
                                    </Border.Effect>
                                </Border>
                                <ListBox x:Name="NatoliOrderListListBox" DockPanel.Dock="Top" Grid.Row="2" Panel.ZIndex="2" ScrollViewer.HorizontalScrollBarVisibility="Disabled" ScrollViewer.VerticalScrollBarVisibility="Hidden" HorizontalContentAlignment="Stretch" BorderThickness="0" Background="Transparent" MaxHeight="800">
                                    <ListBox.ItemContainerStyle>
                                        <Style TargetType="ListBoxItem">
                                            <Setter Property="Template">
                                                <Setter.Value>
                                                    <ControlTemplate TargetType="ListBoxItem">
                                                        <ContentPresenter/>
                                                    </ControlTemplate>
                                                </Setter.Value>
                                            </Setter>
                                        </Style>
                                    </ListBox.ItemContainerStyle>
                                    <ListBox.ItemTemplate>
                                        <DataTemplate>
                                            <ToggleButton x:Name="ToggleButton" PreviewMouseDown="ToggleButton_SingleClick">
                                                <ToggleButton.Style>
                                                    <Style TargetType="{x:Type ToggleButton}">
                                                        <Setter Property="Template">
                                                            <Setter.Value>
                                                                <ControlTemplate TargetType="{x:Type ToggleButton}">
                                                                    <Grid Margin="0,0">
                                                                        <Grid.Background>
                                                                            <Binding ElementName="OrderNumber" Path="Text" Converter="{StaticResource backgroundConverter}" ConverterParameter="NatoliOrderList" NotifyOnSourceUpdated="True"/>
                                                                        </Grid.Background>
                                                                        <Border x:Name="TopBorder" BorderBrush="{StaticResource Bright}" BorderThickness="0,0,0,0" CornerRadius="0" IsHitTestVisible="True" Margin="0,0"/>
                                                                        <Border x:Name="BottomBorder" BorderBrush="{StaticResource Bright}" BorderThickness="0,0,0,1" CornerRadius="0" IsHitTestVisible="True" Margin="0,0"/>
                                                                        <Grid Background="Transparent" ShowGridLines="False" Height="28" Margin="6,0">
                                                                            <Grid.ColumnDefinitions>
                                                                                <ColumnDefinition SharedSizeGroup="Checkbox" Width="35"/>
                                                                                <ColumnDefinition SharedSizeGroup="OrderNumber" Width="80"/>
                                                                                <ColumnDefinition Width="*"/>
                                                                                <ColumnDefinition SharedSizeGroup="ShipDate" Width="100"/>
                                                                                <ColumnDefinition SharedSizeGroup="Rush" Width="55"/>
                                                                                <ColumnDefinition SharedSizeGroup="OnHold" Width="90"/>
                                                                                <ColumnDefinition SharedSizeGroup="Rep" Width="50"/>
                                                                            </Grid.ColumnDefinitions>
                                                                            <CheckBox x:Name="CheckBox" Grid.Column="0" Checked="CheckBox_Checked" Unchecked="CheckBox_Unchecked">
                                                                                <!-- IsChecked="{Binding ElementName=HeaderCheckBox, Path=IsChecked, Mode=OneWay}"-->
                                                                                <CheckBox.IsChecked>
                                                                                    <MultiBinding Converter="{StaticResource checkBoxConverter}" ConverterParameter="Order">
                                                                                        <!--<Binding RelativeSource="{RelativeSource Self}" Mode="OneTime"/>-->
                                                                                        <Binding ElementName="OrderNumber" Path="Text"/>
                                                                                        <Binding ElementName="HeaderCheckBox" Path="IsChecked" Mode="OneWay"/>
                                                                                    </MultiBinding>
                                                                                </CheckBox.IsChecked>
                                                                            </CheckBox>
                                                                            <TextBlock x:Name="OrderNumber" Tag="OrderNumber" Text="{Binding OrderNo}" Style="{StaticResource ModuleTextBlock}" Grid.Column="1"/>
                                                                            <TextBlock Tag="CustomerName" Text="{Binding Customer}" Style="{StaticResource ModuleTextBlock}" Grid.Column="2"/>
                                                                            <TextBlock Tag="ShipDate" Text="{Binding ShipDate, StringFormat='{}{0:MM/dd/yyyy}'}" Style="{StaticResource ModuleTextBlock}" Grid.Column="3"/>
                                                                            <TextBlock Tag="Rush" Text="{Binding Rush}" Style="{StaticResource ModuleTextBlock}" Grid.Column="4" TextWrapping="NoWrap"/>
                                                                            <TextBlock Tag="OnHold" Text="{Binding OnHold}" Style="{StaticResource ModuleTextBlock}" Grid.Column="5" TextWrapping="NoWrap"/>
                                                                            <TextBlock Tag="Rep" Text="{Binding RepInitials}" Style="{StaticResource ModuleTextBlock}" Grid.Column="6" TextWrapping="NoWrap"/>
                                                                        </Grid>
                                                                        <Grid.Triggers>
                                                                            <EventTrigger RoutedEvent="MouseEnter">
                                                                                <BeginStoryboard Name="Highlight" Storyboard="{StaticResource listBoxHighlight}"/>
                                                                            </EventTrigger>
                                                                            <EventTrigger RoutedEvent="PreviewMouseDown">
                                                                                <BeginStoryboard Name="HighlightDown" Storyboard="{StaticResource listBoxHighlight}"/>
                                                                            </EventTrigger>
                                                                            <EventTrigger RoutedEvent="MouseLeave">
                                                                                <StopStoryboard BeginStoryboardName="Highlight"/>
                                                                            </EventTrigger>
                                                                        </Grid.Triggers>
                                                                    </Grid>
                                                                    <ControlTemplate.Triggers>
                                                                        <!--<Trigger Property="IsChecked" Value="True">
                                                                            <Setter Property="IsChecked" TargetName="CheckBox" Value="True"/>
                                                                        </Trigger>-->
                                                                    </ControlTemplate.Triggers>
                                                                </ControlTemplate>
                                                            </Setter.Value>
                                                        </Setter>
                                                    </Style>
                                                </ToggleButton.Style>
                                            </ToggleButton>
                                        </DataTemplate>
                                    </ListBox.ItemTemplate>
                                </ListBox>
                                <Border Grid.Row="3">
                                    <Border Panel.ZIndex="3" BorderBrush="White" BorderThickness="1,1,0,0" CornerRadius="0">
                                        <Border.Effect>
                                            <DropShadowEffect BlurRadius="8" ShadowDepth="3" Direction="135" Color="LightGray" Opacity=".75"/>
                                        </Border.Effect>
                                    </Border>
                                </Border>
                                <DockPanel LastChildFill="True" DockPanel.Dock="Top" Height="30" Grid.Row="3">
                                    <TextBox Name="SearchTextBox" Style="{StaticResource SearchBox}" DockPanel.Dock="Left" Width="Auto" TextChanged="SearchTextBox_TextChanged"/>
                                    <StackPanel Orientation="Horizontal" DockPanel.Dock="Right">
                                        <!--<Button x:Name="SearchButton" Style="{StaticResource SearchButton}" Margin="2,0" HorizontalAlignment="Right"/>
                                        <Button x:Name="WindowButton" Style="{StaticResource WindowButton}" Margin="2,0" HorizontalAlignment="Right"/>
                                        <Button x:Name="ExportButton" Style="{StaticResource ExportButton}" Margin="2,0" HorizontalAlignment="Right"/>
                                        <Button x:Name="ArchiveButton" Style="{StaticResource ArchiveButton}" Margin="2,0" HorizontalAlignment="Right"/>
                                        <Button x:Name="ExpandButton" Style="{StaticResource ExpandButton}" Margin="2,0" HorizontalAlignment="Right"/>
                                        <Button x:Name="CollapseButton" Style="{StaticResource CollapseButton}" Margin="2,0" HorizontalAlignment="Right" Visibility="Visible"/>-->
                                    </StackPanel>
                                    <TextBlock Text="{Binding ElementName=NatoliOrderListListBox, Path=Items.Count, StringFormat='Count: {0}'}" DockPanel.Dock="Right" Style="{StaticResource ModuleTextBlock}" HorizontalAlignment="Right" Margin="0,0,10,0"/>
                                </DockPanel>
                            </Grid>
                            <Grid.Triggers>
                            </Grid.Triggers>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <!--#endregion-->

        <!--#region List Box Note Window -->
        <Style x:Key="ListBox" TargetType="ListBox">
            <Setter Property="SnapsToDevicePixels" Value="true" />
            <Setter Property="OverridesDefaultStyle" Value="true" />
            <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Auto" />
            <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto" />
            <Setter Property="ScrollViewer.CanContentScroll" Value="true"/>
            <Setter Property="SelectionMode" Value="Extended"/>
            <Setter Property="BorderBrush" Value="{StaticResource Medium}"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="ListBox">
                        <Border Name="Border" BorderThickness="1" CornerRadius="3" BorderBrush="{StaticResource Medium}" Background="Transparent">
                            <ScrollViewer Margin="0" Focusable="false">
                                <StackPanel Margin="2" IsItemsHost="True"/>
                            </ScrollViewer>
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsEnabled" Value="False">
                                <Setter TargetName="Border" Property="Background" Value="{StaticResource DisabledBackgroundBrush}"/>
                                <Setter TargetName="Border" Property="BorderBrush" Value="{StaticResource DisabledBorderBrush}"/>
                            </Trigger>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter TargetName="Border" Property="BorderBrush" Value="{StaticResource Dark}"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style x:Key="ListBoxItem" TargetType="ListBoxItem">
            <Setter Property="SnapsToDevicePixels" Value="true" />
            <Setter Property="OverridesDefaultStyle" Value="true" />
            <Setter Property="Background" Value="Transparent"/>
            <Setter Property="Foreground">
                <Setter.Value>
                    <SolidColorBrush Color="{StaticResource Dark.Color}" Opacity="1"/>
                </Setter.Value>
            </Setter>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="ListBoxItem">
                        <Label x:Name="label" Padding="2" SnapsToDevicePixels="True" Background="{TemplateBinding Background}" Foreground="{TemplateBinding Foreground}">
                            <ContentPresenter/>
                        </Label>
                        <ControlTemplate.Triggers>
                            <MultiTrigger>
                                <!-- MouseOver and not Selected  -->
                                <MultiTrigger.Conditions>
                                    <Condition Property="IsMouseOver" Value="True" />
                                    <Condition Property="IsSelected" Value="False" />
                                </MultiTrigger.Conditions>
                                <MultiTrigger.EnterActions>
                                    <BeginStoryboard>
                                        <Storyboard>
                                            <ColorAnimation Duration="0:0:0.02" Storyboard.TargetName="label" Storyboard.TargetProperty="Background.Color" To="{StaticResource Bright.Color}" />
                                            <ColorAnimation Duration="0:0:0.02" Storyboard.TargetName="label" Storyboard.TargetProperty="Foreground.Color" To="{StaticResource Dark.Color}" />
                                        </Storyboard>
                                    </BeginStoryboard>
                                </MultiTrigger.EnterActions>
                                <MultiTrigger.ExitActions>
                                    <BeginStoryboard>
                                        <Storyboard FillBehavior="Stop">
                                            <ColorAnimation Duration="0:0:0.08" Storyboard.TargetName="label" Storyboard.TargetProperty="Background.Color" To="{StaticResource Light.Color}" />
                                            <ColorAnimation Duration="0:0:0.08" Storyboard.TargetName="label" Storyboard.TargetProperty="Foreground.Color" To="{StaticResource Dark.Color}" />
                                        </Storyboard>
                                    </BeginStoryboard>
                                </MultiTrigger.ExitActions>
                            </MultiTrigger>
                            <MultiTrigger>
                                <!-- Selected and not focused  -->
                                <MultiTrigger.Conditions>
                                    <Condition Property="IsSelected" Value="True" />
                                    <Condition Property="IsFocused" Value="False" />
                                </MultiTrigger.Conditions>
                                <MultiTrigger.EnterActions>
                                    <BeginStoryboard>
                                        <Storyboard>
                                            <ColorAnimation Duration="0:0:0.02" Storyboard.TargetName="label" Storyboard.TargetProperty="Background.Color" To="{StaticResource Light.Color}"/>
                                            <ColorAnimation Duration="0:0:0.02" Storyboard.TargetName="label" Storyboard.TargetProperty="Foreground.Color" To="{StaticResource Dark.Color}"/>
                                        </Storyboard>
                                    </BeginStoryboard>
                                </MultiTrigger.EnterActions>
                                <MultiTrigger.ExitActions>
                                    <BeginStoryboard>
                                        <Storyboard FillBehavior="Stop">
                                            <ColorAnimation Duration="0:0:0.10" Storyboard.TargetName="label" Storyboard.TargetProperty="Background.Color" To="Transparent"/>
                                            <ColorAnimation Duration="0:0:0.10" Storyboard.TargetName="label" Storyboard.TargetProperty="Foreground.Color" To="{StaticResource Dark.Color}"/>
                                        </Storyboard>
                                    </BeginStoryboard>
                                </MultiTrigger.ExitActions>
                                <!-- Selected and focused  -->
                            </MultiTrigger>
                            <MultiTrigger>
                                <MultiTrigger.Conditions>
                                    <Condition Property="IsSelected" Value="True"/>
                                    <Condition Property="IsFocused" Value="True"/>
                                </MultiTrigger.Conditions>
                                <MultiTrigger.EnterActions>
                                    <BeginStoryboard>
                                        <Storyboard>
                                            <ColorAnimation Duration="0:0:0.02" Storyboard.TargetName="label" Storyboard.TargetProperty="Background.Color" To="{StaticResource Light.Color}" />
                                            <ColorAnimation Duration="0:0:0.02" Storyboard.TargetName="label" Storyboard.TargetProperty="Foreground.Color" To="{StaticResource Dark.Color}"/>
                                        </Storyboard>
                                    </BeginStoryboard>
                                </MultiTrigger.EnterActions>
                                <MultiTrigger.ExitActions>
                                    <BeginStoryboard>
                                        <Storyboard FillBehavior="Stop">
                                            <ColorAnimation Duration="0:0:0.10" Storyboard.TargetName="label" Storyboard.TargetProperty="Background.Color" To="{StaticResource Bright.Color}" />
                                            <ColorAnimation Duration="0:0:0.10" Storyboard.TargetName="label" Storyboard.TargetProperty="Foreground.Color" To="{StaticResource Dark.Color}"/>
                                        </Storyboard>
                                    </BeginStoryboard>
                                </MultiTrigger.ExitActions>
                            </MultiTrigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <!--#endregion-->
        
