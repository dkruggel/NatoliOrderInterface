                                                                            <Setter Property="IsChecked" TargetName="CheckBox" Value="True"/>
                                                                        </Trigger>-->
                                                                    </ControlTemplate.Triggers>
                                                                </ControlTemplate>
                                                            </Setter.Value>
                                                        </Setter>
                                                    </Style>
                                                </ToggleButton.Style>
                                            </ToggleButton>
