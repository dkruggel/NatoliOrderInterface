        <Style x:Key="RotateButtonIcon" TargetType="Button">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Border Name="Border" Cursor="Hand" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" CornerRadius="6" Margin="{TemplateBinding Margin}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                            <Image Source="{Binding Tag, RelativeSource={RelativeSource TemplatedParent}}" RenderTransformOrigin="0.5,0.5">
                                <Image.RenderTransform>
                                    <TransformGroup>
                                        <RotateTransform x:Name="Orientation"/>
                                    </TransformGroup>
                                </Image.RenderTransform>
                                <Image.Triggers>
                                    <EventTrigger RoutedEvent="PreviewMouseDown">
                                        <BeginStoryboard x:Name="RotateIt">
                                            <Storyboard>
                                                <DoubleAnimation Storyboard.TargetName="Orientation" Storyboard.TargetProperty="Angle" By="360" Duration="0:0:.5"/>
                                                <DoubleAnimation Storyboard.TargetProperty="(Image.Opacity)" From="0.1" To="1" Duration="0:0:.5"/>
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </EventTrigger>
                                </Image.Triggers>
                            </Image>
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <EventTrigger RoutedEvent="MouseEnter">
                    <BeginStoryboard x:Name="Highlight">
                        <Storyboard>
                            <ColorAnimation Storyboard.TargetProperty="(Button.Background).(SolidColorBrush.Color)" Duration="0" To="LightGray" FillBehavior="HoldEnd"/>
                            <DoubleAnimation Storyboard.TargetProperty="(Button.Background).(Opacity)" To="0.5" FillBehavior="HoldEnd" Duration="0"/>
