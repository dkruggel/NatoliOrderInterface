        <!--#region Image Buttons -->
        <Style x:Key="ShakeButtonIcon" TargetType="Button">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Border Name="Border" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" CornerRadius="6" Margin="{TemplateBinding Margin}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                            <Image Source="{Binding Tag, RelativeSource={RelativeSource TemplatedParent}}" Cursor="Hand">
                                <Image.RenderTransform>
                                    <TransformGroup>
                                        <TranslateTransform x:Name="Position"/>
                                    </TransformGroup>
                                </Image.RenderTransform>
                                <Image.Triggers>
                                    <EventTrigger RoutedEvent="MouseEnter">
                                        <BeginStoryboard x:Name="ShakeIt">
                                            <Storyboard>
                                                <DoubleAnimationUsingKeyFrames Storyboard.TargetName="Position" Storyboard.TargetProperty="X" RepeatBehavior="2x">
                                                    <EasingDoubleKeyFrame KeyTime="0:0:0.035" Value="0"/>
                                                    <EasingDoubleKeyFrame KeyTime="0:0:0.07" Value="1"/>
                                                    <EasingDoubleKeyFrame KeyTime="0:0:.105" Value="0"/>
                                                    <EasingDoubleKeyFrame KeyTime="0:0:.14" Value="-1"/>
                                                    <EasingDoubleKeyFrame KeyTime="0:0:.175" Value="0"/>
                                                </DoubleAnimationUsingKeyFrames>
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </EventTrigger>
                                    <EventTrigger RoutedEvent="MouseLeave">
                                        <StopStoryboard BeginStoryboardName="ShakeIt"/>
                                    </EventTrigger>
                                </Image.Triggers>
                            </Image>
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <EventTrigger RoutedEvent="MouseEnter">
                    <BeginStoryboard x:Name="Highlight">
                        <Storyboard>
                            <ColorAnimation Storyboard.TargetProperty="(Button.Background).(SolidColorBrush.Color)" Duration="0" To="LightGray" FillBehavior="HoldEnd"/>
                            <DoubleAnimation Storyboard.TargetProperty="(Button.Background).(Opacity)" To="0.5" FillBehavior="HoldEnd" Duration="0"/>
                        </Storyboard>
                    </BeginStoryboard>
                </EventTrigger>
                <EventTrigger RoutedEvent="MouseLeave">
                    <BeginStoryboard>
                        <Storyboard>
