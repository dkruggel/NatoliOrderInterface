                    </BeginStoryboard>
                </EventTrigger>
                <EventTrigger RoutedEvent="MouseLeave">
                    <BeginStoryboard>
                        <Storyboard>
                            <ColorAnimation Storyboard.TargetProperty="(Button.Background).(SolidColorBrush.Color)" Duration="0" To="Transparent" FillBehavior="HoldEnd"/>
                            <DoubleAnimation Storyboard.TargetProperty="(Button.Background).(Opacity)" To="1" FillBehavior="HoldEnd" Duration="0"/>
                        </Storyboard>
                    </BeginStoryboard>
                </EventTrigger>
            </Style.Triggers>
