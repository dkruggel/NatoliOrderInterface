<Window x:Class="NatoliOrderInterface.BarcodeLocationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NatoliOrderInterface"
        mc:Ignorable="d"
        Title="BarcodeLocationWindow" WindowStartupLocation="CenterOwner" UseLayoutRounding="True">
    <Grid>
        <Border x:Name="ImageBorder">
            <Image x:Name="LocationImage" Stretch="Fill"/>
        </Border>
    </Grid>
</Window>
