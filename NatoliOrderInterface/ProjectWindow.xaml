                                                        <System:String>Special Dust Cup Groove Courtoy</System:String>
                                                        <System:String>Special Dust Cup Groove</System:String>
                                                        <System:String>Special Bellows Groove Kilian</System:String>
                                                        <System:String>Special Bellows Groove</System:String>
                                                    </ComboBox>
                                                </DockPanel>
                                                <DockPanel Margin="3,0,0,0" Grid.Column="1">
                                                    <TextBlock Text="Lower Type:" Style="{StaticResource NormalTextBlock}" VerticalAlignment="Center"/>
