                                                        <System:String>Dust Cup Groove</System:String>
                                                        <System:String>Bellows Groove</System:String>
                                                        <System:String>Combo Groove</System:String>
                                                        <System:String>"O" Ring Groove</System:String>
                                                        <System:String>Special Dust Cup Groove Courtoy</System:String>
                                                        <System:String>Special Dust Cup Groove</System:String>
                                                        <System:String>Special Bellows Groove Kilian</System:String>
                                                        <System:String>Special Bellows Groove</System:String>
                                                    </ComboBox>
                                                </DockPanel>
                                            </Grid>

                                            <CheckBox x:Name="UpperGroove" Grid.Row="2" VerticalContentAlignment="Center" Margin="0,4,0,4" VerticalAlignment="Center">
                                                <TextBlock Text="Upper" Style="{StaticResource NormalTextBlock}"/>
                                            </CheckBox >
                                            <CheckBox x:Name="LowerGroove" Grid.Row="3" VerticalContentAlignment="Center" Margin="0,4,0,4" VerticalAlignment="Center">
                                                <TextBlock Text="Lower" Style="{StaticResource NormalTextBlock}"/>
