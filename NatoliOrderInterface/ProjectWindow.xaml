                                <ComboBox x:Name="MultiTipStyle" Grid.Column="3" Style="{StaticResource ComboBoxAlignsLeft}" Margin="4,-2,0,0" IsEditable="False" HorizontalContentAlignment="Center" IsEnabled="{Binding Path=IsChecked, RelativeSource={RelativeSource AncestorType={x:Type CheckBox}}}" SelectedIndex="-1">
