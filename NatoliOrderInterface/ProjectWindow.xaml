<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NatoliOrderInterface"
        xmlns:System="clr-namespace:System;assembly=System.Runtime" x:Name="ProjectWindowXAML" x:Class="NatoliOrderInterface.ProjectWindow"
        mc:Ignorable="d"
        Title="" Height="1070" Width="1200" Background="{StaticResource BackgroundBrush}" Closed="ProjectWindowXAML_Closed" UseLayoutRounding="True" Activated="ProjectWindowXAML_Activated" Closing="ProjectWindowXAML_Closing">
    <Window.Resources>
        <System:String x:Key="UnitsText">in</System:String>

        <Storyboard x:Key="ColorFade">
            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="BorderBrush">
                <DiscreteObjectKeyFrame KeyTime="0" Value="#43eb34"/>
                <DiscreteObjectKeyFrame KeyTime="0:0:5" Value="Black"/>
            </ObjectAnimationUsingKeyFrames>
        </Storyboard>
    </Window.Resources>
    <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled" CanContentScroll="False">
        <Grid>
            <DockPanel>
                <!-- Link Quote Header  -->
                <Grid DockPanel.Dock="Top">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition Width="230"/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Border Grid.ColumnSpan="3" Grid.RowSpan="3" BorderBrush="Black" BorderThickness="0,1" VerticalAlignment="Bottom" HorizontalAlignment="Stretch"/>
                    <Border x:Name="LinkedToQuoteBorder" Grid.Column="1" Grid.RowSpan="3" BorderBrush="Black" BorderThickness="2,0,2,2">
                        <DockPanel VerticalAlignment="Stretch" LastChildFill="False" HorizontalAlignment="Center" Margin="2,6">
                            <TextBlock x:Name="Quote_WO_Label"  Text="Quote#:" Width="43" VerticalAlignment="Center" Style="{StaticResource NormalTextBlock}"/>
                            <TextBox Style="{StaticResource TextBox}" x:Name="QuoteNumber" Text="" Width="48" VerticalAlignment="Center"/>
                            <TextBlock x:Name="Dash" Text="-" Width="5" VerticalAlignment="Center" Style="{StaticResource NormalTextBlock}"/>
                            <TextBox Style="{StaticResource TextBox}" x:Name="QuoteRevNumber"  Text="" Width="20" VerticalAlignment="Center"/>
                            <Button Style="{StaticResource ButtonIcon}" Tag="{StaticResource linkDrawingImage}" x:Name="LinkQuoteButton" BorderBrush="Transparent" Background="Transparent" MaxHeight="24" VerticalAlignment="Center" Margin="6,0,3,0" Click="LinkQuoteButton_Click" ToolTip="Link Quote"/>
                            <Button Style="{StaticResource ButtonIcon}" Tag="{StaticResource window_alt_plus_DrawingImage}" BorderBrush="Transparent" Background="Transparent" MaxHeight="24" Click="OpenQuoteButton_Click" Name="OpenQuoteButton" Visibility="Visible" VerticalAlignment="Center" ToolTip="Open Quote"/>
                        </DockPanel>
                    </Border>                    
                    <DockPanel Grid.Row="0" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="6,0,0,0">
                        <TextBlock Text="CSR:" FontSize="12" VerticalAlignment="Center" FontWeight="Bold" Style="{StaticResource BoldTextBlock}"/>
                        <TextBlock x:Name="CSR" Text="Alex" VerticalAlignment="Center" FontSize="12" Margin="4,0,0,0" Style="{StaticResource NormalTextBlock}"/>
                        <TextBlock Text="Return To:" FontSize="12" VerticalAlignment="Center" Margin="10,0,0,0" Style="{StaticResource BoldTextBlock}"/>
                        <ComboBox x:Name="ReturnToCSR" Style="{StaticResource ComboBoxAlignsCenter}" Width="100" IsEditable="False" VerticalAlignment="Center" Margin="4,0,0,0"  BorderThickness="0" Background="Transparent" Padding="6,1,5,1"/>
                        <TextBlock Text="Created:" Style="{StaticResource BoldTextBlock}" Margin="10,0,0,0"/>
                        <TextBlock x:Name="EnteredDate" Text="" MinWidth="40" Style="{StaticResource NormalTextBlock}" Margin="4,0,0,0"/>
                    </DockPanel>
                    <DockPanel Grid.Row="2" Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="6,0,0,12">
                        <TextBlock Text="Rev'd:" Style="{StaticResource BoldTextBlock}" Margin="0,0,0,0"/>
                        <TextBlock x:Name="RevisionDate" Text="" MinWidth="40" Style="{StaticResource NormalTextBlock}" Margin="4,0,0,0"/>
                        <TextBlock Text="By:" Style="{StaticResource BoldTextBlock}" Margin="20,0,0,0"/>
                        <TextBlock x:Name="RevisedBy" Text="" MinWidth="40" Style="{StaticResource NormalTextBlock}" Margin="4,0,0,0"/>
                    </DockPanel>
                    <DockPanel Grid.Row="0" Grid.Column="3" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="0,0,4,0">
                        <TextBlock Text="Unit Of Measure:" FontSize="12" VerticalAlignment="Center" Style="{StaticResource BoldTextBlock}"/>
                        <ComboBox x:Name="UnitOfMeasure" Style="{StaticResource ComboBoxAlignsCenter}" IsEditable="False" Width="60" VerticalAlignment="Center" Margin="4,0,0,0" BorderThickness="0" Background="Transparent" Padding="6,1,5,1">
                            <ComboBoxItem Content="IN" IsSelected="True"/>
                            <ComboBoxItem Content="MM"/>
                            <ComboBoxItem Content="DUAL" />
                        </ComboBox>
                        <TextBlock Text="Ref. Quote#:" VerticalAlignment="Center" Margin="20,0,0,0" Style="{StaticResource NormalTextBlock}"/>
                        <TextBox Style="{StaticResource TextBox}" x:Name="ReferenceQuoteNumber" Text="" Width="48" VerticalAlignment="Center" Margin="4,0,0,0" HorizontalContentAlignment="Right"/>
                        <TextBlock Text="-" Width="5" VerticalAlignment="Center" Style="{StaticResource NormalTextBlock}"/>
                        <TextBox Style="{StaticResource TextBox}" x:Name="ReferenceQuoteRevNumber" Text="" Width="20" VerticalAlignment="Center" Margin="0,0,0,0"/>
                    </DockPanel>
                    <DockPanel Grid.Row="3" Grid.Column="3" HorizontalAlignment="Right" VerticalAlignment="Center"  Margin="0,0,4,0">
                        <TextBlock Text="Ref. Project#:" VerticalAlignment="Top" FontSize="12" Margin="4,0,0,0" Style="{StaticResource NormalTextBlock}"/>
                        <TextBox Style="{StaticResource TextBox}" x:Name="ReferenceProjectNumber" Text="" Width="48" VerticalAlignment="Center" Margin="4,0,0,0" HorizontalContentAlignment="Right"/>
                        <TextBlock Text="-" Width="5" VerticalAlignment="Center" Style="{StaticResource NormalTextBlock}"/>
                        <TextBox Style="{StaticResource TextBox}" x:Name="ReferenceProjectRevNumber" Text="" Width="20" VerticalAlignment="Center"/>
                        <TextBlock Text="Ref. Order#:" VerticalAlignment="Top" FontSize="12" Margin="20,0,0,0" Style="{StaticResource NormalTextBlock}"/>
                        <TextBox Style="{StaticResource TextBox}" x:Name="RefOrderNumber" Text="" Width="48" VerticalAlignment="Center" Margin="4,0,0,0" HorizontalContentAlignment="Right" MaxLength="6"/>
                    </DockPanel>
                    <Border Grid.Row="1" Grid.Column="0" BorderBrush="Black" VerticalAlignment="Center" BorderThickness="0.5"/>
                    <Border Grid.Row="1" Grid.Column="3" BorderBrush="Black" VerticalAlignment="Center" BorderThickness="0.5"/>
                </Grid>
                <!-- Top Half -->
                <Grid DockPanel.Dock="Top">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="150"/>
                        <ColumnDefinition/>
                        <ColumnDefinition Width="200"/>
                    </Grid.ColumnDefinitions>
                    <Border x:Name="ProjectNavigation" Visibility="Visible" Grid.Column="0" BorderBrush="Black" BorderThickness="0,2,2,2" Margin="0,-2,0,0" CornerRadius="0,0,0,0">
                        <StackPanel x:Name="RoutingStackPanel">
                            <Grid VerticalAlignment="Center" HorizontalAlignment="Center" Margin="0,10,0,0">
                                <Border x:Name="SwitchProjectType" BorderBrush="Black" BorderThickness="2" Cursor="Hand" Background="{StaticResource BackgroundBrush}" MouseUp="SwitchProjectRoutingType_MouseUp">
                                    <DockPanel Margin="4,0" LastChildFill="False">
                                        <Image Height="20" Source="Switch_Logo.PNG" HorizontalAlignment="Center"/>
                                        <TextBlock x:Name="CurrentProjectType" Text="TABLETS" Margin="8,0,0,0" VerticalAlignment="Center" FontSize="16" Style="{StaticResource BoldTextBlock}" Padding="0,0,0,2"/>
                                    </DockPanel>
                                </Border>
                            </Grid>
                            <Button Style="{StaticResource Button}" Height="24" x:Name="StartButton" Content="Start" Click="StartButton_Click" IsEnabled="True"/>
                            <Button Style="{StaticResource Button}" Height="24" x:Name="FinishButton" Content="Finish" Click="FinishButton_Click"/>
                            <Button Style="{StaticResource Button}" Height="24" x:Name="SubmitButton" Content="Submit" Click="SubmitButton_Click"/>
                            <Button Style="{StaticResource Button}" Height="24" x:Name="CheckButton" Content="Check" Click="CheckButton_Click"/>
                            <Button Style="{StaticResource Button}" Height="24" x:Name="PutOnHoldButton" Content="Put On Hold" Click="PutOnHoldButton_Click"/>
                            <Button Style="{StaticResource Button}" Height="24" x:Name="CancelButton" Content="Cancel" Click="CancelButton_Click"/>
                            <Button Style="{StaticResource Button}" Height="24" x:Name="ReviseButton" Content="Revise" Click="ReviseButton_Click"/>
                        </StackPanel>
                    </Border>
                    <!-- One will be hidden ^ or v -->
                    <Border x:Name="CreationBorder" Visibility="Hidden" Grid.Column="0" BorderBrush="Black" BorderThickness="0,2,2,2" Margin="0,-2,0,0" CornerRadius="0,0,0,0">
                        <StackPanel>
                            <Button Style="{StaticResource Button}" Height="24" x:Name="CreateButton" Content="Create" Click="CreateButton_Click"/>
                            <Button Style="{StaticResource Button}" Height="24" x:Name="SaveButton" Content="Save" Click="SaveButton_Click"/>
                            <Button Style="{StaticResource Button}" Height="24" x:Name="CancelCreateButton" Content="Cancel" Click="CancelCreateButton_Click"/>
                        </StackPanel>
                    </Border>
                    <StackPanel Grid.Column="1" HorizontalAlignment="Stretch">
                        <Grid Margin="0,10" HorizontalAlignment="Stretch">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <StackPanel Margin="4,0" Grid.Column="0" VerticalAlignment="Top">
                                <TextBlock Text="CUSTOMER" Margin="0,0,0,4" Style="{StaticResource BoldTextBlock}"/>
                                <Border BorderBrush="Black" BorderThickness="0,1,0,1" Margin="0,-4,0,0">
                                    <Grid Margin="0,4,0,6">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="54"/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="24"/>
                                            <RowDefinition Height="24"/>
                                        </Grid.RowDefinitions>
                                        <TextBlock Text="Number:" VerticalAlignment="Center" Grid.Row="0" Grid.Column="0" Style="{StaticResource NormalTextBlock}"/>
                                        <TextBox Style="{StaticResource TextBox}" x:Name="CustomerNumber" Text="" Width="60" VerticalAlignment="Center" HorizontalAlignment="Left"  Grid.Row="0" Grid.Column="1" TextChanged="CustomerNumber_TextChanged" HorizontalContentAlignment="Right"/>
                                        <TextBlock Text="Name:" VerticalAlignment="Center" Grid.Row="1" Grid.Column="0" Style="{StaticResource NormalTextBlock}"/>
                                        <TextBox Style="{StaticResource TextBox}" x:Name="CustomerName" Text="" HorizontalAlignment="Stretch" VerticalAlignment="Center" Grid.Row="1" Grid.Column="1"/>
                                    </Grid>
                                </Border>
                            </StackPanel>
                            <StackPanel Margin="4,0" Grid.Column="1" VerticalAlignment="Top" >
                                <TextBlock Text="SHIP TO" FontWeight="Bold" Margin="0,0,0,4" Style="{StaticResource BoldTextBlock}"/>
                                <Border BorderBrush="Black" BorderThickness="0,1,0,1" Margin="0,-4,0,0">
                                    <Grid Margin="0,4,0,6">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="54"/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="24"/>
                                            <RowDefinition Height="24"/>
                                        </Grid.RowDefinitions>
                                        <TextBlock Text="Number:" VerticalAlignment="Center" Grid.Row="0" Grid.Column="0" Style="{StaticResource NormalTextBlock}"/>
                                        <DockPanel HorizontalAlignment="Left"  Grid.Row="0" Grid.Column="1">
                                            <TextBox Style="{StaticResource TextBox}" x:Name="ShipToNumber" Text="" Width="60" VerticalAlignment="Center"  TextChanged="CustomerNumber_TextChanged" HorizontalContentAlignment="Right"/>
                                            <TextBlock Text="-" Width="5" VerticalAlignment="Center" Style="{StaticResource NormalTextBlock}"/>
                                            <TextBox Style="{StaticResource TextBox}" x:Name="ShipToLocNumber" Text="" Width="20" VerticalAlignment="Center"/>
                                        </DockPanel>
                                        <TextBlock Text="Name:" VerticalAlignment="Center" Grid.Row="1" Grid.Column="0" Style="{StaticResource NormalTextBlock}"/>
                                        <TextBox Style="{StaticResource TextBox}" x:Name="ShipToName" Text="" HorizontalAlignment="Stretch" VerticalAlignment="Center" Grid.Row="1" Grid.Column="1"/>
                                    </Grid>
                                </Border>
                            </StackPanel>
                            <StackPanel Margin="4,0" Grid.Column="2" VerticalAlignment="Top">
                                <TextBlock Text="END USER" FontWeight="Bold" Margin="0,0,0,4" Style="{StaticResource BoldTextBlock}"/>
                                <Border BorderBrush="Black" BorderThickness="0,1,0,1" Margin="0,-4,0,0">
                                    <Grid Margin="0,4,0,6">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="54"/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="24"/>
                                            <RowDefinition Height="24"/>
                                        </Grid.RowDefinitions>
                                        <TextBlock Text="Number:" VerticalAlignment="Center" Grid.Row="0" Grid.Column="0" Style="{StaticResource NormalTextBlock}"/>
                                        <DockPanel HorizontalAlignment="Left"  Grid.Row="0" Grid.Column="1">
                                            <TextBox Style="{StaticResource TextBox}" x:Name="EndUserNumber" Text="" Width="60" VerticalAlignment="Center"  TextChanged="CustomerNumber_TextChanged" HorizontalContentAlignment="Right"/>
                                            <TextBlock Text="-" Width="5" VerticalAlignment="Center" Style="{StaticResource NormalTextBlock}"/>
                                            <TextBox Style="{StaticResource TextBox}" x:Name="EndUserLocNumber" Text="" Width="20" VerticalAlignment="Center"/>
                                        </DockPanel>
                                        <TextBlock Text="Name:" VerticalAlignment="Center" Grid.Row="1" Grid.Column="0" Style="{StaticResource NormalTextBlock}"/>
                                        <TextBox Style="{StaticResource TextBox}" x:Name="EndUserName" Text="" HorizontalAlignment="Stretch" VerticalAlignment="Center" Grid.Row="1" Grid.Column="1"/>
                                    </Grid>
                                </Border>
                            </StackPanel>
                        </Grid>
                        <Grid Margin="0,0" HorizontalAlignment="Stretch">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="5*"/>
                                <ColumnDefinition Width="5*"/>
                                <ColumnDefinition Width="10*"/>
                            </Grid.ColumnDefinitions>
                            <StackPanel Margin="4,0" Grid.Column="0" VerticalAlignment="Top" Grid.ColumnSpan="2">
                                <Grid Margin="0,0,0,12">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="54"/>
                                        <ColumnDefinition/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="32"/>
                                        <RowDefinition Height="32"/>
                                    </Grid.RowDefinitions>
                                    <TextBlock Text="Product:" VerticalAlignment="Center" Grid.Row="0" Grid.Column="0" Style="{StaticResource NormalTextBlock}"/>
                                    <TextBox Style="{StaticResource TextBox}" x:Name="Product" Text="" TextWrapping="WrapWithOverflow" VerticalAlignment="Center" HorizontalAlignment="Stretch"  Grid.Row="0" Grid.Column="1" TextChanged="CustomerNumber_TextChanged"/>
                                    <TextBlock Text="Attention:" VerticalAlignment="Center" Grid.Row="1" Grid.Column="0"  Style="{StaticResource NormalTextBlock}"/>
                                    <TextBox Style="{StaticResource TextBox}" x:Name="Attention" Text="" VerticalAlignment="Center" HorizontalAlignment="Stretch"  Grid.Row="1" Grid.Column="1" TextChanged="CustomerNumber_TextChanged"/>
                                </Grid>
                                <Border BorderBrush="Black" BorderThickness="0,2,0,2" Margin="-4,0,-4,0">
                                    <Grid Height="176" Margin="0,0,0,0" ShowGridLines="False">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                        </Grid.RowDefinitions>
                                        <Grid Grid.ColumnSpan="3">
                                            <TextBlock x:Name="ArchivedOrInactive" TextWrapping="Wrap" Margin="4,0,0,0" Text="" FontSize="20" FontWeight="Bold" Opacity="1" Foreground="Red"/>
                                        </Grid>
                                        <Border Grid.ColumnSpan="3" BorderBrush="Black" BorderThickness="0,0,0,2">
                                            <StackPanel VerticalAlignment="Center" >
                                                <TextBlock Text="DUE DATE" Style="{StaticResource NormalTextBlock}" FontWeight="SemiBold" FontSize="12" HorizontalAlignment="Center"/>
                                                <ComboBox x:Name="DueDate" Style="{StaticResource ComboBoxAlignsCenter}" IsEditable="False" HorizontalContentAlignment="Center" Margin="30,0" SelectionChanged="DueDate_SelectionChanged"/>
                                                <TextBlock x:Name="DueDateWarning" Text="" Style="{StaticResource NormalTextBlock}" HorizontalAlignment="Center" Margin="0,2,0,0"/>
                                            </StackPanel>
                                        </Border>
                                        <Border BorderBrush="Black" Grid.Row="1" Grid.Column="0" BorderThickness="0,0,2,0" Margin="0,0,0,0">
                                            <CheckBox x:Name="Priority"  VerticalAlignment="Center" HorizontalAlignment="Center" IsChecked="False" VerticalContentAlignment="Center">
                                                <TextBlock Text="PRIORITY" Style="{StaticResource PriorityTextBlock}" TextAlignment="Left"/>
                                            </CheckBox>
                                        </Border>
                                        <Border BorderBrush="Black" Grid.Column="1" Grid.Row="1" BorderThickness="0,0,2,0" Margin="0,0,0,0">
                                            <CheckBox x:Name="TabletsRequired" VerticalAlignment="Center" HorizontalAlignment="Center" IsChecked="False" Margin="6,0,0,0" VerticalContentAlignment="Center" Click="TabletsRequired_Click">
                                                <TextBlock Text="TABLETS" Style="{StaticResource TabletsTextBlock}" TextAlignment="Left"/>
                                            </CheckBox>
                                        </Border>
                                        <Grid VerticalAlignment="Center" Grid.Column="2" Grid.Row="1">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition/>
                                                <ColumnDefinition Width="Auto"/>
                                                <ColumnDefinition/>
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition/>
                                                <RowDefinition Height="8"/>
                                                <RowDefinition/>
                                            </Grid.RowDefinitions>
                                            <CheckBox DockPanel.Dock="Top" Grid.Row="0" Grid.Column="1" x:Name="ToolsRequired" VerticalAlignment="Center" HorizontalAlignment="Left" IsChecked="False" VerticalContentAlignment="Center" Click="ToolsRequired_Click">
                                                <TextBlock Text="TOOLS" Style="{StaticResource ToolsTextBlock}" TextAlignment="Left" />
                                            </CheckBox>
                                            <CheckBox DockPanel.Dock="Top" Grid.Row="2" Grid.Column="1" x:Name="MultiTipSketch" VerticalAlignment="Center" HorizontalAlignment="Left" VerticalContentAlignment="Center" HorizontalContentAlignment="Stretch" Click="MultiTipSketch_Click">
                                                <TextBlock Text="M-TIP SKETCH" Style="{StaticResource MultiTipSketchTextBlock}" VerticalAlignment="Center"/>
                                            </CheckBox>
                                        </Grid>
                                    </Grid>
                                </Border>
                            </StackPanel>
                            <Border Grid.Column="2" BorderBrush="Black" BorderThickness="2,2,0,2" CornerRadius="3,0,0,0">
                                <DockPanel Margin="4,0">
                                    <Grid DockPanel.Dock="Top" Height="74">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="85"/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition Width="50"/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition Width="10"/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                        </Grid.RowDefinitions>
                                        <TextBlock Text="Machine No:" VerticalAlignment="Center" Style="{StaticResource NormalTextBlock}"/>
                                        <TextBox Style="{StaticResource TextBox}" x:Name="MachineNumber" Text="" TextWrapping="WrapWithOverflow" VerticalAlignment="Bottom" Grid.Column="1" Margin="0,0,10,0" TextChanged="MachineNumber_TextChanged"/>
                                        <TextBlock Text="Die Size:" VerticalAlignment="Center" Grid.Column="2" Style="{StaticResource NormalTextBlock}"/>
                                        <TextBlock x:Name="DieODPlaceholder" Text="O.D. in." VerticalAlignment="Bottom" HorizontalAlignment="Center" Grid.Column="3" Foreground="LightGray" IsEnabled="False"/>
                                        <TextBox Style="{StaticResource TextBox}" x:Name="DieOD" Text="" VerticalAlignment="Bottom" HorizontalAlignment="Stretch" Grid.Column="3" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" GotFocus="TextBox_GotFocus" LostFocus="TextBox_LostFocus"/>
                                        <TextBlock Text="X" Grid.Column="4" VerticalAlignment="Bottom" HorizontalAlignment="Center" Style="{StaticResource NormalTextBlock}"/>
                                        <TextBlock x:Name="DieOLPlaceholder" Text="O.L. in." VerticalAlignment="Bottom" HorizontalAlignment="Center" Grid.Column="5" Foreground="LightGray" IsEnabled="False"/>
                                        <TextBox Style="{StaticResource TextBox}" x:Name="DieOL" Text="" VerticalAlignment="Bottom" HorizontalAlignment="Stretch" Grid.Column="5" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" GotFocus="TextBox_GotFocus" LostFocus="TextBox_LostFocus"/>
                                        <TextBlock Text="Machine Desc:" Grid.Row="1" Grid.ColumnSpan="2" VerticalAlignment="Center" Style="{StaticResource NormalTextBlock}"/>
                                        <TextBox Style="{StaticResource TextBox}" x:Name="MachineDescription" Text="" TextWrapping="WrapWithOverflow" VerticalAlignment="Center" HorizontalAlignment="Stretch" Grid.ColumnSpan="5" Grid.Row="1" Grid.Column="1"/>
                                    </Grid>
                                    <Grid DockPanel.Dock="Top">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="20"/>
                                            <RowDefinition Height="*"/>
                                        </Grid.RowDefinitions>
                                        <Border Margin="-4,0" Grid.Row="0" CornerRadius="0" BorderThickness="0,2,0,0" BorderBrush="Black" Height="20">
                                            <TextBlock Text="NOTES" HorizontalAlignment="Center" Style="{StaticResource BoldTextBlock}"/>
                                        </Border>
                                        <RichTextBox x:Name="Notes" Grid.Row="1" Style="{StaticResource RichTextBox}" Foreground="{StaticResource Dark}" VerticalContentAlignment="Top" VerticalAlignment="Top" Height="138" BorderThickness=".5" Margin="-2,0,-2,0" AcceptsReturn="True" AcceptsTab="True" VerticalScrollBarVisibility="Auto"/>
                                    </Grid>
                                </DockPanel>
                            </Border>
                        </Grid>
                    </StackPanel>
                    <Border Grid.Column="3" BorderBrush="Black" BorderThickness="2,2,0,2" Margin="0,-2,0,0" CornerRadius="0,0,0,0" HorizontalAlignment="Stretch">
                        <DockPanel HorizontalAlignment="Stretch" Margin="5,0,5,0">
                            <Button Style="{StaticResource Button}" Height="24" DockPanel.Dock="Top" x:Name="ProjectFolderButton" Content="Project Folder" Click="ProjectFolderButton_Click"/>
                            <Button Style="{StaticResource Button}" Height="24" DockPanel.Dock="Top" x:Name="QuoteFolderButton" Content="Quote Folder" Click="QuoteFolderButton_Click"/>
                            <Button Style="{StaticResource Button}" Height="24" DockPanel.Dock="Top" x:Name="TabletDrawingsButton" Content="Tablet Drawings" Click="TabletDrawingsButton_Click"/>
                            <Button Style="{StaticResource Button}" Height="24" DockPanel.Dock="Top" x:Name="SpecificationsButton" Content="Specifications" Click="SpecificationsButton_Click"/>
                            <Border DockPanel.Dock="Bottom" BorderThickness="0,2,0,0" Margin="-5,0" BorderBrush="Black" x:Name="AttachFilesBorder" AllowDrop="True" DragEnter="AttachFilesBorder_DragEnter" DragLeave="AttachFilesBorder_DragLeave" DragOver="AttachFilesBorder_DragOver" Drop="AttachFilesBorder_Drop">
                                <DockPanel VerticalAlignment="Center">
                                    <TextBlock DockPanel.Dock="Top" Text="FILES" HorizontalAlignment="Center" FontWeight="Bold" Margin="0,10,0,0" Style="{StaticResource BoldTextBlock}"/>
                                    <!--<Image DockPanel.Dock="Top" Margin="4,0,4,0" Source="{StaticResource add_item_altDrawingImage}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="60"/>-->
                                    <ListBox x:Name="FilesListBox" Style="{DynamicResource FilePicker}" DockPanel.Dock="Top" Height="146" MouseDoubleClick="file_MouseDoubleClick" PreviewKeyUp="file_PreviewKeyUp" SelectionMode="Single">
                                        <!--<ListBox.ItemTemplate>
                                            <DataTemplate>
                                                <StackPanel Name="stackPanel">
                                                    <Border Name="button" Padding="0" BorderBrush="Transparent" Background="Transparent" ToolTip="{Binding Item3}">
                                                        <DockPanel LastChildFill="True">
                                                            <Image Name="image" Height="{Binding  ActualHeight, ElementName=textBlock}" Source="{DynamicResource file_emptyDrawingImage}"></Image>
                                                            <TextBlock Name="textBlock" Text="{Binding Item1}" Foreground="{StaticResource Dark}" Height="18" Margin="2,0,0,0"/>
                                                        </DockPanel>
                                                    </Border>
                                                </StackPanel>
