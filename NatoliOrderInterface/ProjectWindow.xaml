                                                </Grid>
                                            </CheckBox>
                                            <CheckBox x:Name="DieInsert" Grid.Row="4" VerticalContentAlignment="Center" Margin="0,4,0,4" HorizontalContentAlignment="Stretch" VerticalAlignment="Center">
