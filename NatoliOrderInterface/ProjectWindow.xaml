                                                </Grid>
                                            </CheckBox>
                                            <CheckBox x:Name="DiePlate" Grid.Row="5" VerticalContentAlignment="Center" Margin="0,4,0,4" HorizontalContentAlignment="Stretch" VerticalAlignment="Center">
