                                                    <System:String>Manesty Head</System:String>
                                                    <System:String>European Head</System:String>
                                                    <System:String>Fette European Head</System:String>
                                                    <System:String>Korsch European Head</System:String>
                                                    <System:String>Fette EU1-441 Head</System:String>
                                                    <System:String>Special Head Configuration</System:String>
                                                    <System:String>Kilian European Head</System:String>
                                                    <System:String>Cam-Slot Head</System:String>
                                                    <System:String>Fette EU28-441 Head</System:String>
                                                    <System:String>Domed Head-Old Style</System:String>
                                                    <System:String>Fette EU1-441/28 Head</System:String>
                                                    <System:String>Fette EU1-441/28N Head</System:String>
                                                    <System:String>Domed Head-Special</System:String>
                                                    <System:String>Fette FS19 Head</System:String>
                                                    <System:String>Fette Fs12 Head</System:String>
                                                    <System:String>Natoli MDT-25 Head</System:String>
                                                    <System:String>Natoli MDT-19 Head</System:String>
                                                    <System:String>ISO European Head</System:String>
                                                </ComboBox>
                                            </DockPanel>
                                            <CheckBox x:Name="CarbideTips" Grid.Row="2" VerticalContentAlignment="Center" Margin="0,4,0,4" VerticalAlignment="Center">
