                                                    <DataTrigger Binding="{Binding Path=Item3}" Value=".XLSM">
                                                        <Setter TargetName="image" Property="Source" Value="{DynamicResource file_tableDrawingImage}"/>
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding Path=Item3}" Value=".xlsx">
                                                        <Setter TargetName="image" Property="Source" Value="{DynamicResource file_tableDrawingImage}"/>
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding Path=Item3}" Value=".XLSX">
                                                        <Setter TargetName="image" Property="Source" Value="{DynamicResource file_tableDrawingImage}"/>
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding Path=Item3}" Value=".pdf">
                                                        <Setter TargetName="image" Property="Source" Value="{DynamicResource file_pdfDrawingImage}"/>
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding Path=Item3}" Value=".PDF">
                                                        <Setter TargetName="image" Property="Source" Value="{DynamicResource file_pdfDrawingImage}"/>
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding Path=Item3}" Value=".msg">
                                                        <Setter TargetName="image" Property="Source" Value="{DynamicResource mailDrawingImage}"/>
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding Path=Item3}" Value=".MSG">
                                                        <Setter TargetName="image" Property="Source" Value="{DynamicResource mailDrawingImage}"/>
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding Path=Item3}" Value=".jpg">
                                                        <Setter TargetName="image" Property="Source" Value="{DynamicResource imageDrawingImage}"/>
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding Path=Item3}" Value=".JPG">
                                                        <Setter TargetName="image" Property="Source" Value="{DynamicResource imageDrawingImage}"/>
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding Path=Item3}" Value=".png">
                                                        <Setter TargetName="image" Property="Source" Value="{DynamicResource imageDrawingImage}"/>
                                                    </DataTrigger>
