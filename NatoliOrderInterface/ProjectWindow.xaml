                                            <Border Grid.Row="3" Margin="-4,0,-8,0" BorderBrush="Black" BorderThickness="0,2,0,0">
                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Center" Text="MACHINE NOTES" Style="{StaticResource BoldTextBlock}"/>
                                            </Border>
                                            <Grid Grid.Row="4" Grid.RowSpan="2">
                                                <TextBox x:Name="MachineNotes" Style="{StaticResource TextBox}" Height="71" Margin="0,0,-4,0" Foreground="{StaticResource Dark}" VerticalContentAlignment="Top" VerticalAlignment="Bottom" BorderThickness=".5" VerticalScrollBarVisibility="Auto" Text="" HorizontalAlignment="Stretch"/>
                                            </Grid>
                                        </Grid>
                                    </Border>
                                </Grid>
                            </Border>
                        </DockPanel>
                    </TabItem>
                    <TabItem Style="{StaticResource TabItem}" x:Name="MultiTipSketchTabItem" Header="MULTI-TIP" Padding="8,4,8,4" Background="Gray" IsEnabled="True">
                        <DockPanel Visibility="{Binding Visibility, ElementName=MultiTipSketch}" IsEnabled="{Binding IsEnabled, ElementName=MultiTipSketchTabItem}" LastChildFill="False">
                            <Grid DockPanel.Dock="Top" HorizontalAlignment="Center" Margin="0,20">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="65"/>
                                    <ColumnDefinition Width="100"/>
                                    <ColumnDefinition Width="100"/>
                                    <ColumnDefinition Width="200"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Style="{StaticResource NormalTextBlock}" Text="Sketch ID:"/>
                                <TextBox Style="{StaticResource TextBox}" x:Name="SketchID" Grid.Column="1"  HorizontalContentAlignment="Center" Margin="0,0,10,0" TextChanged="SketchID_TextChanged"/>
