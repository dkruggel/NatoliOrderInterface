<Window x:Class="NatoliOrderInterface.PDFOrderingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NatoliOrderInterface"
        mc:Ignorable="d"
        Title="PDFOrderingWindow" Width="800" WindowStartupLocation="CenterScreen" Topmost="True" Background="{StaticResource BackgroundBrush}" MaxHeight="550" UseLayoutRounding="True">
    <DockPanel LastChildFill="False">
        <TextBlock DockPanel.Dock="Top" Text="Specify .pdf Order" Style="{StaticResource BoldTextBlock}" FontSize="24" HorizontalAlignment="Center"/>
        <Border DockPanel.Dock="Top" Margin="0,20,0,20" BorderBrush="{StaticResource Focused.Border}" BorderThickness="0" MaxHeight="400">
            <ListBox x:Name="ListBox1" Margin="20,0" ScrollViewer.VerticalScrollBarVisibility="Auto"/>
        </Border>
        <Grid x:Name="ButtonGrid" HorizontalAlignment="Right" DockPanel.Dock="Bottom">
        </Grid>
    </DockPanel>
</Window>
